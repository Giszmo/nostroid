{
	"extends": "./.svelte-kit/tsconfig.json",
	"exclude": ["node_modules/*", "__sapper__/*", "public/*"],
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"lib": ["DOM", "DOM.Iterable", "WebWorker"],
		"sourceMap": true,
		"strict": true
	}
}
